{
	"welcomeFile": "/soldOrder/index.html",
	"authenticationMethod": "none",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/soldOrder/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "/api-proxy(.*)",
		"destination": "retailSoldOrder-node-backend",
		"csrfProtection": false,
		"authenticationType": "none"
	}, {
		"source": "/node(.*)",
		"destination": "retailSoldOrder-node-backend",
		"csrfProtection": false,
		"authenticationType": "none"
	}]
}