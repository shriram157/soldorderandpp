{
	"welcomeFile": "/soldOrder/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/soldOrder/(.*)$",
		"target": "$1",
		"localDir": "webapp",
		"authenticationType": "none"
	}, {
		"source": "/apiProxy(.*)",
		"destination": "retailSoldOrder-node-backend",
		"authenticationType": "none",
		"csrfProtection": false
	}, {
		"source": "/node(.*)",
		"destination": "retailSoldOrder-node-backend",
		"authenticationType": "none",
		"csrfProtection": false
	}, {
		"source": "/userDetails(.*)",
		"destination": "retailSoldOrder-node-backend",
		"authenticationType": "xsuaa",
		"csrfProtection": false
	}]
}