							</layoutData>
						</Label>
						<u:FileUploader id="fileUploader"/>
						<HBox>
							<items>
								<Label text="{i18n>Comments}" wrapping="true" class="sapUiSmallMarginTop sapUiSmallMarginEnd" required="true">
									<layoutData>
										<l:GridData span="L1 M3 S6"/>
									</layoutData>
								</Label>
								<TextArea id="comments_NFSO" maxLength="30">
									<layoutData>
										<l:GridData span="L2 M3 S6"/>
									</layoutData>
								</TextArea>
							</items>
							<layoutData>
