<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:t="sap.ui.table" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" controllerName="toyota.ca.SoldOrder.controller.FleetSoldOrderSummary" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page showHeader="false">
				<core:Fragment fragmentName="toyota.ca.SoldOrder.view.fragments.ToolHeader" type="XML"/>
				<content>
					<VBox>
						<items>
							<Label id="label_FleetSoldOrderSummaryid" text="{i18n>fleetSoldOrderSummary}"
								class="sapUiTinyMarginTop sapUiTinyMarginBegin sapUiTinyMarginBottom fontsize"></Label>
						
							<VBox class="marginCorners VBox_color">
								<items>
									<f:SimpleForm columnsL="3" columnsM="3" editable="true" emptySpanL="0" emptySpanM="0" labelSpanL="4" labelSpanM="4"
										layout="ResponsiveGridLayout" >
										<f:content>
											<VBox>
												<Label text="{i18n>status}" class="TableLabel">
													<layoutData>
														<l:GridData span="L2 M12 S12"/>
													</layoutData>
												</Label>
												<MultiComboBox id="mcb_status_FSOS"  selectionChange="_refresh" items="{ path: '/FSOSummary_Status', sorter: { path: 'key' } }"
													class="sapUiMediumMarginEnd">
													<core:Item key="{key}" text="{text}"/>
													<layoutData>
														<l:GridData span="L1 M6 S12"/>
													</layoutData>
												</MultiComboBox>
											</VBox>
											<VBox>
												<Label text="{i18n>OrderType}" class="TableLabel">
													<layoutData>
														<l:GridData span="L2 M12 S12"/>
													</layoutData>
												</Label>
												<MultiComboBox id="mcb_ordTyp_FSOS"  selectionChange="_refresh" items="{ path: '/FSOSummary_OrderType', sorter: { path: 'key' } }"
													class="sapUiMediumMarginEnd">
													<core:Item key="{key}" text="{text}"/>
													<layoutData>
														<l:GridData span="L1 M6 S12"/>
													</layoutData>
												</MultiComboBox>
											</VBox>
											<VBox id="vbx_dealer_FSOS">
												<Label id="lbl_dealer_FSOS" visible="true" text="{i18n>dealer}" class="TableLabel">
													<layoutData>
														<l:GridData span="L2 M12 S12"/>
													</layoutData>
												</Label>
												<MultiComboBox id="mcb_dealer_FSOS"  visible="true" items="{LoginUserModel>/BpDealerModel}" selectionChange="_refresh"  class="sapUiMediumMarginEnd">
													<core:Item key="{LoginUserModel>BusinessPartnerKey}" text="{parts: [{path:'LoginUserModel>BusinessPartnerKey'},{path:'LoginUserModel>BusinessPartnerName'}],formatter: 'toyota.ca.SoldOrder.util.formatter.formatdealers'}"/>
													<layoutData>
														<l:GridData span="L2 M12 S12"/>
													</layoutData>
												</MultiComboBox>
											</VBox>
										</f:content>
									</f:SimpleForm>
								
								</items>
							</VBox>
							
							<t:Table id="tbl_FSOS" selectionMode="None" rows="{mainservices>/SO_FLEET_HeaderSet}" enableColumnReordering="false" alternateRowColors="true"
								enableColumnFreeze="false" class="marginCorners" visibleRowCount="10">
								<t:columns>
									<t:Column width="11.11%" resizable="false" >
										<Label text="{i18n>orderNum}" class="TableLabel"/>
										<t:template>
											<Link text="{mainservices>ZsoFltReqNo}" press="_navToSoldOrder"/>
										</t:template>
									</t:Column>
									<t:Column width="11.11%" resizable="false" >
										<Label id="tbl_lbl_dealer_FSOS" visible="true" text="{i18n>dealer}" class="TableLabel"/>
										<t:template>
											<!--,{path:'/Products/0/Category'}] , formatter: 'toyota.ca.SoldOrder.util.formatter.formatDealer'}-->
											<Text id="tbl_val_dealer_FSOS" visible="true" text="{path:'mainservices>ZzdealerCode',formatter: 'toyota.ca.SoldOrder.util.formatter.formatdealerno'}"/>
										</t:template>
									</t:Column>
									<t:Column width="11.11%" resizable="false" >
										<Label text="{i18n>status}" class="TableLabel"/>
										<t:template>
											<Text text="{mainservices>ZsoFltStatus}"/>
										</t:template>
									</t:Column>
									<t:Column width="11.11%" resizable="false" >
										<Label text="{i18n>FanNumber}" class="TableLabel"/>
										<t:template>
											<Text text="{mainservices>ZfanNo}"/>
										</t:template>
									</t:Column>
									<t:Column width="11.11%" resizable="false" >
										<Label text="{i18n>CustName}" class="TableLabel"/>
										<t:template>
											<Text text="{mainservices>ZzendcuName}"/>
										</t:template>
									</t:Column>
									<t:Column width="11.11%" resizable="false" >
										<Label text="{i18n>PONumber}" class="TableLabel"/>
										<t:template>
											<Text text="{mainservices>ZpoNumber}"/>
										</t:template>
									</t:Column>
									<t:Column width="11.11%" resizable="false" >
										<Label text="{i18n>orderType}" class="TableLabel"/>
										<t:template>
											<Text text="{mainservices>Zadd1}"/>
										</t:template>
									</t:Column>
									<t:Column width="11.11%" resizable="false" >
										<Label text="{i18n>zoneAppNumber}" class="TableLabel"/>
										<t:template>
											<Text text="{mainservices>ZzoneApproval}"/>
										</t:template>
									</t:Column>
									<t:Column width="11.11%" resizable="false" >
										<Label text="{i18n>totalVehicles}" class="TableLabel"/>
										<t:template>
											<Text text="{mainservices>ZtotalVehQty}"/>
										</t:template>
									</t:Column>
								</t:columns>
							</t:Table>
						</items>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>