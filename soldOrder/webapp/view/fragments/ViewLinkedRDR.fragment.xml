<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:tnt="sap.tnt">
	<Dialog title="{i18n>ViewLinkedRDR}">
		<content>
				<f:SimpleForm class="FormTitle VisibleStyle" labelSpanXL="3" labelSpanL="3" layout="ResponsiveGridLayout" emptySpanXL="0" emptySpanL="0"
					emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false" labelSpanM="12" labelSpanS="12"
					adjustLabelSpan="false">
					<f:content>
						<Label text="{i18n>VIN}"></Label>
						<Text text="{path:'VLinkedRDRModel>/results/0/Vhvin'}"/>
						<Label text="{i18n>CustomerName}"></Label>
						<Text text="{path:'VLinkedRDRModel>/results/0/CustName1'}"/>
						<Label text="{i18n>VAddress}"></Label>
						<Text text="{path:'VLinkedRDRModel>/results/0/Street'}"/>
						<Label text="{i18n>VCity}"></Label>
						<Text text="{path:'VLinkedRDRModel>/results/0/City1'}"/>
						<Label text="{i18n>InvoiceDate}"></Label>
						<Text text="{path:'VLinkedRDRModel>/results/0/Invoice_date',formatter:'toyota.ca.SoldOrder.util.formatter._CreatedOnFormatter'}"/>
						<Label text="{i18n>RDRDate}"></Label>
						<Text text="{path:'VLinkedRDRModel>/results/0/Rdr_date',formatter:'toyota.ca.SoldOrder.util.formatter._CreatedOnFormatter'}"/>
				</f:SimpleForm>
			</VBox>
		</content>
		<endButton>
			<Button text="{i18n>Close}" press="onCloseViewLinkedRDRDialog"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>