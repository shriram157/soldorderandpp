<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:tnt="sap.tnt">
	<Dialog title="{i18n>ViewLinkedRDR}" contentWidth="50%" contentHeight="50%">
		<content>
			<VBox>
				<f:SimpleForm>
					<f:content>
						<VBox>
							<items>
								<HBox>
									<items>
										<Label text="{i18n>VIN}"  class="sapUiSmallMarginBegin sapUiSmallMarginTop sapUiSmallMarginEnd"></Label>
										<Text text="{path:'VLinkedRDRModel>/results/0/Vhvin'}" class="sapUiSmallMarginTop"/>
									</items>
								</HBox>
								<HBox>
									<items>
										<Label text="{i18n>CustomerName}"  class="sapUiSmallMarginBegin sapUiSmallMarginTop sapUiSmallMarginEnd"></Label>
										<Text text="{path:'VLinkedRDRModel>/results/0/CustName1'}" class="sapUiSmallMarginTop"/>
									</items>
								</HBox>
								<HBox>
									<items>
										<Label text="{i18n>VAddress}"  class="sapUiSmallMarginBegin sapUiSmallMarginTop sapUiSmallMarginEnd"></Label>
										<Text text="{path:'VLinkedRDRModel>/results/0/Street'}" class="sapUiSmallMarginTop"/>
									</items>
								</HBox>
								<HBox>
									<items>
										<Label text="{i18n>VCity}" class="sapUiSmallMarginBegin sapUiSmallMarginTop sapUiSmallMarginEnd"></Label>
										<Text text="{path:'VLinkedRDRModel>/results/0/City1'}" class="sapUiSmallMarginTop"/>
									</items>
								</HBox>
								<HBox>
									<items>
										<Label text="{i18n>InvoiceDate}" class="sapUiSmallMarginBegin sapUiSmallMarginTop sapUiSmallMarginEnd"></Label>
										<Text text="{path:'VLinkedRDRModel>/results/0/Invoice_date',formatter:'toyota.ca.SoldOrder.util.formatter._CreatedOnFormatter'}" class="sapUiSmallMarginTop"/>
									</items>
								</HBox>
								<HBox>
									<items>
										<Label text="{i18n>RDRDate}" class="sapUiSmallMarginBegin sapUiSmallMarginTop sapUiSmallMarginEnd"></Label>
										<Text text="{path:'VLinkedRDRModel>/results/0/Rdr_date',formatter:'toyota.ca.SoldOrder.util.formatter._CreatedOnFormatter'}" class="sapUiSmallMarginTop"/>
									</items>
								</HBox>
							</items>
						</VBox>
					</f:content>
				</f:SimpleForm>
			</VBox>
		</content>
		<endButton>
			<Button text="{i18n>Close}" press="onCloseViewLinkedRDRDialog"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>