<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form"
	controllerName="toyota.ca.SoldOrder.controller.ChangeSoldOrderRequest" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page showHeader="false">
				<core:Fragment fragmentName="toyota.ca.SoldOrder.view.fragments.ToolHeader" type="XML"/>
				<content>
					<Label text="{i18n>changeSoldOrdertitle}" class="sapUiTinyMarginTop sapUiTinyMarginBegin fontsize"></Label>
					<f:SimpleForm class="FormTitle" editable="true" title="" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="4"
						labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2"
						singleContainerFullSize="false">
						<f:content>
							<core:Title text=""/>
							<MessageStrip text="{i18n>VehicleInformation}" class="msgStripFormat msgStripCss "></MessageStrip >
							<Label text="{i18n>modelYear}"/>
							<Text text="{mainservices>Zzmoyr}"/>
							<Label text="{i18n>Series}"/>
							<Text text="{mainservices>Zzseries}"/>
							<Label text="{i18n>Model}"/>
							<ComboBox id="model_CSOR" items="{ path: 'VechileModel>/ZC_BRAND_MODEL_DETAIL' }" required="true" selectedKey="{mainservices>Zzmodel}">
								<core:Item key="{VechileModel>Model}" text="{VechileModel>Model}-{VechileModel>ENModelDesc}"/>
							</ComboBox>
							<Label text="{i18n>Suffix}"/>
							<ComboBox id="suffix_CSOR" items="{ path: 'VechileModel>/zc_configuration'}" required="true" selectedKey="{mainservices>Zzsuffix}">
								<core:Item key="{VechileModel>Suffix}" text="{parts: [{path:'VechileModel>SuffixDescriptionEN'},{path:'VechileModel>TrimInteriorColor'}],formatter: 'toyota.ca.SoldOrder.util.formatter.formatSuffix'}"/>
							</ComboBox>
							<Label text="{i18n>APX}"/>
							<ComboBox id="apx_CSOR" items="{path: 'VechileModel>/ZC_PIO_DIO'}" required="true" selectedKey="{mainservices>Zzapx}">
								<core:Item key="{VechileModel>zzapx}" text="{VechileModel>zzapx}"/>
							</ComboBox>
							<Label text="{i18n>Colour}"/>
							<ComboBox id="colour_CSOR" items="{ path: 'VechileModel>/zc_exterior_trim'}" required="true" selectedKey="{mainservices>Zzextcol}">
								<core:Item key="{VechileModel>ExteriorColorCode}" text="{path:'VechileModel>ExteriorDescriptionEN'}"/>
							</ComboBox>
							<Label text="{i18n>ETA}" required="true"/>
							<VBox>
								<items>
									<HBox>
										<items>
											<Label text="{i18n>from}" class="sapUiSmallMarginTop sapUiSmallMarginEnd"/>
											<DatePicker id="etaFrom_CSOR" placeholder="dd/MMM/yy"  dateValue="{mainservices>ZzreqEtaFrom}" change="_handleChangeDate"></DatePicker>
										</items>
									</HBox>
									<HBox>
										<items>
											<Label text="{i18n>to}" class="sapUiSmallMarginTop sapUiMediumMarginEnd"/>
											<DatePicker id="etaTo_CSOR" placeholder="dd/MMM/yy"   dateValue="{mainservices>ZzreqEtaTo}"></DatePicker>
										</items>
									</HBox>
								</items>
							</VBox>
							<core:Title text=""/>
							<MessageStrip text="{i18n>AdditionalInformation}" class="msgStripFormat msgStripCss"></MessageStrip >
							<!--<Label text="{i18n>custname}"/>-->
							<Label text="{i18n>contractDate}"/>
							<Text text="{path:'mainservices>ZcontractDate', type:'sap.ui.model.type.DateTime', formatOptions: { pattern: 'MMMM d, y', strictParsing: false, relative: false }}"/>
							<Label text="{i18n>salesType}"/>
							<Text text="{mainservices>ZsalesType}"/>
							<Label text="{i18n>salesPerName}"/>
							<Text text="{mainservices>Zsalesperson}"/>
							<Label text="{i18n>salesManName}"/>
							<Text text="{mainservices>Zsalesmanager}"/>
							<Label text="{i18n>tradeInModelYr}"/>
							<Text text="{mainservices>ZtradeModelYr}"/>
							<Label text="{i18n>tradeInMake}"/>
							<Text text="{mainservices>ZtradeMake}"/>
							<Label text="{i18n>tradeInModel}"/>
							<Text text="{mainservices>ZtradeModel}"/>
						</f:content>
					</f:SimpleForm>
					<f:SimpleForm class="FormTitle" title="" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="12"
						labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2"
						singleContainerFullSize="false">
						<f:content>
							<core:Title text=""/>
							<MessageStrip text="{i18n>CustomerInformation}" class="msgStripFormat msgStripCss"></MessageStrip >
							<Label  text="{i18n>CustName}"/>
							<Text text="{Customer>/person/firstName}{Customer>/person/familyName}"/>
							<Label text="{i18n>address}"/>
							<Text text="{Customer>/addresses/0/line1}"/>
							<Label text="{i18n>city}"/>
							<Text text="{Customer>/addresses/0/city}"/>
							<Label  text="{i18n>province}"/>
							<Text text="{Customer>/addresses/0/provinceCode}"/>
							<Label  text="{i18n>postalcode}"/>
							<Text text="{Customer>/addresses/0/postalCode}"/>
							<Label  text="{i18n>phnNo}"/>
							<Text text="{Customer>/phones/0/localNumber}"/>
							<Label  text="{i18n>email}"/>
							<Text text="{Customer>/electronicAddresses/0/uriID}"/>
							<Label text="{i18n>driverLic}"/>
							<Text text="{address>/Name}"/>
							<core:Title text=""/>
							<MessageStrip text="{i18n>Comments}" class="msgStripFormat msgStripCss"></MessageStrip >
							<Label text="{i18n>PreSoldOrder}"/>
							<Link text="{mainservices>ZzsoReqNo}"></Link>
							<Label text="{i18n>Comments}"/>
							<TextArea id="Comment" value="{mainservices>Comment}"/>
						</f:content>
					</f:SimpleForm>
					<Button text="{i18n>submit}" press="_onSubmit" type="Emphasized" class="marginCornersRight marginCornersBottom cssShift"></Button>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>
