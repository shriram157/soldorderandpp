<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:t="sap.ui.table"
	controllerName="toyota.ca.SoldOrder.controller.PriceProtectionDetails_Dealer" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page>
				<customHeader>
					<Bar class="sapUiSmallPaddingBeginEnd clCustomHeaderBg">
						<contentLeft>
							<Image id="idLexusLogo" src="images/Lexus_EN.png" width="100px"/>
							<Text text="|" class="sapUiTinyMarginBeginEnd"/>
							<Text class="clCustomFont" text="{i18n>PageTitle}"/>
						</contentLeft>
						<contentRight>
							<Text text="{i18n>soldOrder}" class="sapUiTinyMarginBeginEnd"></Text>
							<Text text="{i18n>priceProtection}" class="sapUiTinyMarginBeginEnd"></Text>
							<!--	<Text text="{i18n>summary}" class="sapUiTinyMarginBeginEnd"></Text>-->
						</contentRight>
					</Bar>
				</customHeader>
				<content>
					<VBox>
						<items>
							<Label id="label_PriceProtectionDetails_Dealerid" text="{i18n>PriceProtectionDetails}"
								class="sapUiTinyMarginTop sapUiTinyMarginBegin sapUiTinyMarginBottom fontsize"></Label>
							<HBox class="marginCorners VBox_color">
								<items>
									<Label text="{i18n>status}" class="sapUiSmallMarginTop TableLabel sapUiTinyMarginEnd"></Label>
									<ComboBox items="{ path: '/PriceProtectionStatus', sorter: { path: 'key' } }" required="true" class="sapUiMediumMarginEnd">
										<core:Item key="{key}" text="{text}"/>
									</ComboBox>
									<Label text="{i18n>OrderType}" class="sapUiSmallMarginTop TableLabel sapUiTinyMarginEnd"></Label>
										<ComboBox items="{ path: '/PriceProtection_OrderType', sorter: { path: 'key' } }" required="true" class="sapUiMediumMarginEnd">
										<core:Item key="{key}" text="{text}"/>
									</ComboBox>
									<Label text="{i18n>dealer}" class="sapUiSmallMarginTop TableLabel sapUiTinyMarginEnd"></Label>
									<ComboBox items="{ path: '/', sorter: { path: 'reason' } }" required="true" class="sapUiMediumMarginEnd">
										<core:Item key="{ProductId}" text="{Name}"/>
									</ComboBox>
								</items>
							</HBox>
							<t:Table id="table_PPD_Dealer" selectionMode="None" rows="{/}" enableColumnReordering="false" alternateRowColors="true"
								enableColumnFreeze="false" class="marginCorners"> <!--'priceProtModel>/zc_campaign_pricing'-->
								<t:columns>
										<t:Column width="11.1%" resizable="false" >
										<Label class="TableLabel" text="{i18n>orderNum}"/>
										<t:template>
												<Link text="{num}"/>
										</t:template>
									</t:Column>
										<t:Column width="11.1%" resizable="false" >
										<Label class="TableLabel" text="{i18n>orderType}"/>
										<t:template>
											<Text text="{Category}"/>
										</t:template>
									</t:Column>
											<t:Column width="11.1%" resizable="false" >
										<Label class="TableLabel" text="{i18n>series}"/>
										<t:template>
											<Text text="{series}"/>
										</t:template>
									</t:Column>
										<t:Column width="11.1%" resizable="false" >
										<Label class="TableLabel" text="{i18n>modelYear}"/>
										<t:template>
											<Text text="{model_year}"/>
										</t:template>
									</t:Column>
										<t:Column width="11.1%" resizable="false" >
										<Label class="TableLabel" text="{i18n>model}"/>
										<t:template>
											<Text text="{Category}"/>
										</t:template>
									</t:Column>
										<t:Column width="11.1%" resizable="false" >
										<Label class="TableLabel" text="{i18n>suffix}"/>
										<t:template>
											<Text text="{Category}"/>
										</t:template>
									</t:Column>
										<t:Column width="11.1%" resizable="false" >
										<Label class="TableLabel" text="{i18n>status}"/>
										<t:template>
											<Text text="{Category}"/>
										</t:template>
									</t:Column>
										<t:Column width="11.1%" resizable="false" >
										<Label class="TableLabel" text="{i18n>OwnershipUploaded}"/>
										<t:template>
											<Text text="{Category}"/>
										</t:template>
									</t:Column>
										<t:Column width="11.1%" resizable="false" >
										<Label class="TableLabel" text="{i18n>CreditMemo}"/>
										<t:template>
											<Text text="{Category}"/>
										</t:template>
									</t:Column>
								</t:columns>
							</t:Table>
						</items>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>